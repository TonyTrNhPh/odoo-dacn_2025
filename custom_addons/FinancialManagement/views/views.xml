<odoo>
    <!-- List View -->
    <record id="view_revenue_list" model="ir.ui.view">
        <field name="name">revenue.list</field>
        <field name="model">revenue</field>
        <field name="arch" type="xml">
            <list string="Revenue">
                <field name="name"/>
                <field name="age"/>
                <field name="gender"/>
                <field name="phone"/>
                <field name="address"/>
            </list>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_revenue_form" model="ir.ui.view">
        <field name="name">revenue.form</field>
        <field name="model">revenue</field>
        <field name="arch" type="xml">
            <form string="Revenue Form">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="age"/>
                        <field name="gender"/>
                        <field name="phone"/>
                        <field name="address"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Action -->
    <record id="action_revenue" model="ir.actions.act_window">
        <field name="name">Revenue</field>
        <field name="res_model">revenue</field>
        <field name="view_mode">list,form</field>
    </record>

    <!-- Menu -->
    <!-- Root Menu -->
    <menuitem id="menu_hospital_management" name="Financial Management" sequence="10"/>

    <!-- Child Menu -->
    <menuitem id="menu_revenue" 
              name="Revenue" 
              parent="menu_hospital_management" 
              action="action_revenue" 
              sequence="10"/>
</odoo>